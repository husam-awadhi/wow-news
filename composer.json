{
    "name": "husam-awadhi/news-web",
    "description": "News Portal",
    "type": "project",
    "require": {
        "php": ">=7.4.15",
        "eftec/routeone": "1.*",
        "monolog/monolog": "^2.2",
        "twig/twig": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
